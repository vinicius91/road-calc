<div class="tab-pane" id="tabContent">
    <p>
        <b>Distancia entre dois pontos - Decimal</b>
    </p>
    <!--INICIO DO FORMULÁRIO-->
    <form role="form">
        <div class="box-body">
            <div class="row">
                <h4>Ponto 1</h4>
                <div class="form-group col-lg-4">
                    <label for="CoordXPt1">Coordenada X</label>
                    <input type="number" class="form-control" id="CoordXPt1" step="0.01" required>
                </div>
                <div class="form-group col-lg-4">
                    <label for="CoordYPt1">Coordenada Y</label>
                    <input type="number" class="form-control" id="CoordYPt1" step="0.01" required>
                </div>
            </div>
            <hr />
            <div class="row">
                <h4>Ponto 2</h4>
                <div class="form-group col-lg-4">
                    <label for="CoordXPt2">Coordenada X</label>
                    <input type="number" class="form-control" id="CoordXPt2" step="0.01" required>
                </div>
                <div class="form-group col-lg-4">
                    <label for="CoordYPt2">Coordenada Y</label>
                    <input type="number" class="form-control" id="CoordYPt2" step="0.01" required>
                </div>
            </div>
        </div>
        <!-- /.box-body -->

        <div class="box-footer">
            <button type="submit" class="btn btn-primary">Calcular</button>
        </div>
    </form>
    <!--TÉRMINO DO FORMULÁRIO-->
</div>
